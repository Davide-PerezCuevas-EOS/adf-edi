{
	"name": "EDI-DESADV-Pipeline",
	"properties": {
		"activities": [
			{
				"name": "ReadSalesShipmentHeader",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "SELECT\n      SalesShipmentHeader.[No_]                      AS ShipmentNo\n     ,[Sell-to Customer No_]                         AS SellToCustomerNo\n     ,[Bill-to Customer No_]                         AS BillToCustomerNo\n     ,[Bill-to Name]                                 AS BillToName\n     ,[Posting Date]                                 AS PostingDate\n     ,[Shipment Date]                                AS ShipmentDate\n     ,[SalesShipmentHeaderExt].[Shpfy Order No_$ec255f57-31d0-4ca2-b751-f2fa7c745abb] AS ShopifyOrderNo\n\nFROM \n    [CRONUS].[dbo].[CDD$Sales Shipment Header$437dbf0e-84ff-417a-965d-ed2bb9650972] AS SalesShipmentHeader\nINNER JOIN\n    [CRONUS].[dbo].[CDD$Sales Shipment Header$437dbf0e-84ff-417a-965d-ed2bb9650972$ext] AS SalesShipmentHeaderExt\nON\n    SalesShipmentHeader.No_ = SalesShipmentHeaderExt.No_\n    \n",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "ShipmentNo",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "ShipmentNo",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "SellToCustomerNo",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "SellToCustomerNo",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "BillToCustomerNo",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "BillToCustomerNo",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "BillToName",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "BillToName",
									"type": "String",
									"physicalType": "UTF8"
								}
							},
							{
								"source": {
									"name": "PostingDate",
									"type": "DateTime",
									"physicalType": "datetime"
								},
								"sink": {
									"name": "PostingDate",
									"type": "DateTime",
									"physicalType": "INT_96"
								}
							},
							{
								"source": {
									"name": "ShipmentDate",
									"type": "DateTime",
									"physicalType": "datetime"
								},
								"sink": {
									"name": "ShipmentDate",
									"type": "DateTime",
									"physicalType": "INT_96"
								}
							},
							{
								"source": {
									"name": "ShopifyOrderNo",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "ShopifyOrderNo",
									"type": "String",
									"physicalType": "UTF8"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "BcSqlServer",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SalesShipmentHdrParquet",
						"type": "DatasetReference",
						"parameters": {
							"pDirectory": {
								"value": "@concat(\n  'raw/sales_shipment/',\n  'date=', formatDateTime(utcNow(),'yyyy-MM-dd'),\n  '/partner=', variables('pPartnerCode')\n)",
								"type": "Expression"
							},
							"pFileName": {
								"value": "@concat(\n  'sales_shipment_',\n  variables('pPartnerCode'),\n  '_',\n  formatDateTime(utcNow(),'yyyyMMdd'),\n  '.parquet'\n)",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"variables": {
			"pEdiType": {
				"type": "String"
			},
			"pPartnerCode": {
				"type": "String"
			}
		},
		"annotations": []
	}
}