{
	"name": "dataflow1",
	"properties": {
		"folder": {
			"name": "EDI-DESADV"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SalesShptHeaderCSV",
						"type": "DatasetReference"
					},
					"name": "ReadFromSalesShptHeaderCSV"
				},
				{
					"name": "ReadFromSalesShipmentLine"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "JoinHeaderAndLine"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     skipLines: 1) ~> ReadFromSalesShptHeaderCSV",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> ReadFromSalesShipmentLine",
				"ReadFromSalesShptHeaderCSV, ReadFromSalesShipmentLine join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> JoinHeaderAndLine"
			]
		}
	}
}